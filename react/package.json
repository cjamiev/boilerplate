{
  "name": "sandbox-react",
  "version": "1.0.0",
  "description": "Sandbox for testing and experimenting with react code",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "coverage": "jest --env=jsdom --coverage",
    "test": "jest --env=jsdom --watch",
    "t": "jest __tests__/components/form/SwapSelect --watch"
  },
  "author": "CJV",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.0",
    "connected-react-router": "6.4.0",
    "history": "4.9.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "react-router-dom": "5.0.0",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/node": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@testing-library/react": "^9.1.3",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "redux-logger": "3.0.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "react-app"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.js"
    ],
    "moduleDirectories": [
      "src",
      "__tests__/testHelper",
      "node_modules"
    ],
    "moduleNameMapper": {
      "testHelper(.*)$": "<rootDir>/__tests__/testHelper/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/cssTransformer.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/fileTransformer.js"
    },
    "testPathIgnorePatterns": [
      "__tests__/testHelper"
    ],
    "testRegex": "__tests__/.*\\.(js|jsx)$",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}