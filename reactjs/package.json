{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate code for react",
  "private": true,
  "scripts": {
    "start": "webpack",
    "dev": "webpack-dev-server --config ./config/webpack.config.js",
    "eslint": "eslint --ext .js src/ -f html -o ./eslint.html",
    "eslint-fix": "eslint --fix --ext .js src/",
    "prettier": "prettier --write src/**/*.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^5.0.7",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.12.4",
    "prettier": "^1.15.3",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "optionalDependencies": {
    "mini-css-extract-plugin": "^0.5.0"
  }
}